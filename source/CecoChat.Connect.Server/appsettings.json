{
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "ConfigurationDB": {
    "Endpoints": [
      "localhost:6379"
    ],
    "ConnectRetry": 16,
    "ConnectTimeout": 5000,
    "KeepAlive": 2
  },
  "Jwt": {
    "Secret": "secret that is long and hard to guess",
    "Issuer": "issuer",
    "Audience": "audience"
  },
  "Jaeger": {
    "AgentHost": "localhost",
    "AgentPort": 6831,
    "ExportProcessorType": "Batch",
    "BatchExportScheduledDelayMillis": 2000
  },
  "Swagger": {
    "UseSwagger": true,
    "UseSwaggerUI": true,
    "Url": "/swagger/v1/swagger.json",
    "AddAuthorizationHeader": true,
    "OpenApiInfo": {
      "Title": "CecoChat Connect",
      "Version": "v1",
      "Description": "CecoChat API for Connect 1.0 service"
    }
  }
}
