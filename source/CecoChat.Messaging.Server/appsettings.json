{
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Clients": {
    "SendMessagesHighWatermark": 64
  },
  "ConfigurationDB": {
    "Endpoints": [
      "localhost:6379"
    ],
    "ConnectRetry": 16,
    "ConnectTimeout": 5000,
    "KeepAlive": 2
  },
  "Backend": {
    "Kafka": {
      "BootstrapServers": [
        "localhost:9091",
        "localhost:9092"
      ]
    },
    "MessagesToBackendProducer": {
      "ProducerID": "messages-to-backend",
      "Acks": "All",
      "LingerMs": 1.0,
      "MessageTimeoutMs": 300000,
      "MessageSendMaxRetries": 8
    },
    "MessagesToReceiversConsumer": {
      "ConsumerGroupID": "messages-to-receivers",
      "AutoOffsetReset": "Earliest",
      "EnablePartitionEof": false,
      "AllowAutoCreateTopics": false,
      "EnableAutoCommit": false
    },
    "ServerID": "s1",
    "MessagesTopicName": "messages"
  },
  "Jwt": {
    "Secret": "secret that is long and hard to guess",
    "Issuer": "issuer",
    "Audience": "audience"
  },
  "OtelSampling": {
    "Strategy": "Probability",
    "Probability": 0.1
  },
  "Jaeger": {
    "AgentHost": "localhost",
    "AgentPort": 6831,
    "ExportProcessorType": "Batch",
    "BatchExportScheduledDelayMillis": 2000
  }
}