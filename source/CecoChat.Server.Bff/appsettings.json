{
  "AllowedHosts": "*",
  "ConfigDb": {
    "Name": "config-db",
    "Endpoints": [
      "localhost:6379"
    ],
    "ConnectRetry": 16,
    "ConnectTimeout": 5000,
    "KeepAlive": 2,
    "DefaultDatabase": 0,
    "HealthTimeout": "00:00:05.0"
  },
  "HistoryClient": {
    "Address": "https://localhost:31003",
    "CallTimeout": "00:00:05.0",
    "SocketsHttpHandler": {
      "KeepAlivePingDelay": "00:01:00.0",
      "KeepAlivePingTimeout": "00:00:30.0",
      "EnableMultipleHttp2Connections": true
    },
    "Retry": {
      "RetryCount": 4,
      "InitialBackOff": "00:00:00.500",
      "BackOffMultiplier": 1.5,
      "MaxBackOff": "00:00:02.000",
      "MaxJitterMs": "250"
    },
    "HealthPath": "readyz",
    "HealthTimeout": "00:00:05.0"
  },
  "StateClient": {
    "Address": "https://localhost:31002",
    "CallTimeout": "00:00:05.0",
    "SocketsHttpHandler": {
      "KeepAlivePingDelay": "00:01:00.0",
      "KeepAlivePingTimeout": "00:00:30.0",
      "EnableMultipleHttp2Connections": true
    },
    "Retry": {
      "RetryCount": 4,
      "InitialBackOff": "00:00:00.500",
      "BackOffMultiplier": 1.5,
      "MaxBackOff": "00:00:02.000",
      "MaxJitterMs": "250"
    },
    "HealthPath": "readyz",
    "HealthTimeout": "00:00:05.0"
  },
  "UserClient": {
    "Address": "https://localhost:31005",
    "CallTimeout": "00:00:05.0",
    "SocketsHttpHandler": {
      "KeepAlivePingDelay": "00:01:00.0",
      "KeepAlivePingTimeout": "00:00:30.0",
      "EnableMultipleHttp2Connections": true
    },
    "Retry": {
      "RetryCount": 4,
      "InitialBackOff": "00:00:00.500",
      "BackOffMultiplier": 1.5,
      "MaxBackOff": "00:00:02.000",
      "MaxJitterMs": "250"
    },
    "HealthPath": "readyz",
    "HealthTimeout": "00:00:05.0"
  },
  "Jwt": {
    "Secret": "secret that is long and hard to guess",
    "Issuer": "issuer",
    "Audience": "audience",
    "AccessTokenExpiration": "00:30:00.0"
  },
  "Swagger": {
    "UseSwagger": true,
    "UseSwaggerUi": true,
    "Url": "/swagger/v1/swagger.json",
    "AddAuthorizationHeader": true,
    "GroupByApiExplorerGroup": true,
    "OpenApiInfo": {
      "Title": "CecoChat BFF",
      "Version": "v1",
      "Description": "CecoChat API for BFF 1.0"
    }
  },
  "OtelSampling": {
    "Strategy": "Probability",
    "Probability": 0.1
  },
  "Jaeger": {
    "AgentHost": "localhost",
    "AgentPort": 6831,
    "ExportProcessorType": "Batch",
    "BatchExportScheduledDelayMillis": 2000
  },
  "Prometheus": {
    "ScrapeEndpointPath": "/metrics",
    "ScrapeResponseCacheDurationMilliseconds": 0
  }
}
