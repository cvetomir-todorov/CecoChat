{
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Jwt": {
    "Secret": "secret that is long and hard to guess",
    "Issuer": "issuer",
    "Audience": "audience"
  },
  "OtelSampling": {
    "Strategy": "Probability",
    "Probability": 0.1
  },
  "Jaeger": {
    "AgentHost": "localhost",
    "AgentPort": 6831,
    "ExportProcessorType": "Batch",
    "BatchExportScheduledDelayMillis": 2000
  }
}
