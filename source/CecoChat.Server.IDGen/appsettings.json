{
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Config": {
    "ServerID": 0
  },
  "ConfigDB": {
    "Endpoints": [
      "localhost:6379"
    ],
    "ConnectRetry": 16,
    "ConnectTimeout": 5000,
    "KeepAlive": 2,
    "HealthTimeout": "00:00:05.0"
  },
  "OtelSampling": {
    "Strategy": "Probability",
    "Probability": 0.1
  },
  "Jaeger": {
    "AgentHost": "localhost",
    "AgentPort": 6831,
    "ExportProcessorType": "Batch",
    "BatchExportScheduledDelayMillis": 2000
  },
  "Prometheus": {
    "ScrapeEndpointPath": "/metrics",
    "ScrapeResponseCacheDurationMilliseconds": 0
  }
}
