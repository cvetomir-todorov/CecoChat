{
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Config": {
    "ServerId": 0
  },
  "ConfigDb": {
    "Name": "config-db",
    "Endpoints": [
      "localhost:6379"
    ],
    "ConnectRetry": 16,
    "ConnectTimeout": 5000,
    "KeepAlive": 2,
    "HealthTimeout": "00:00:05.0"
  },
  "Telemetry": {
    "Tracing": {
      "Sampling": {
        "Strategy": "Probability",
        "Probability": 0.1
      },
      "Export": {
        "TargetHost": "localhost",
        "TargetPort": 4317,
        "Protocol": "Grpc",
        "ExportProcessorType": "Batch",
        "BatchExportScheduledDelayMillis": 2000
      }
    },
    "Metrics": {
      "Prometheus": {
        "ScrapeEndpointPath": "/metrics",
        "ScrapeResponseCacheDurationMilliseconds": 0
      }
    }
  }
}
