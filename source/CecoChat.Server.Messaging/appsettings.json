{
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Clients": {
    "SendMessagesHighWatermark": 64
  },
  "IDGen": {
    "Generation": {
      "OriginatorID": 1,
      "RefreshIDsCount": 1024,
      "RefreshIDsInterval": "00:00:01.0",
      "GetIDWaitInterval": "00:00:05.0"
    },
    "Communication": {
      "Address": "https://localhost:31004",
      "KeepAlivePingDelay": "00:01:00.0",
      "KeepAlivePingTimeout": "00:00:30.0",
      "CallTimeout": "00:00:10.0"
    },
    "Retry": {
      "RetryCount": 4,
      "InitialBackOff": "00:00:01.0",
      "BackOffMultiplier": 1.5,
      "MaxBackOff": "00:00:05.0",
      "MaxJitterMs": "1000"
    }
  },
  "ConfigDB": {
    "Endpoints": [
      "localhost:6379"
    ],
    "ConnectRetry": 16,
    "ConnectTimeout": 5000,
    "KeepAlive": 2
  },
  "Backplane": {
    "Kafka": {
      "BootstrapServers": [
        "localhost:9091",
        "localhost:9092"
      ]
    },
    "SendProducer": {
      "ProducerID": "send-producer",
      "Acks": "All",
      "LingerMs": 1.0,
      "MessageTimeoutMs": 300000,
      "MessageSendMaxRetries": 8
    },
    "ReceiversConsumer": {
      "ConsumerGroupID": "messaging-receivers-consumer",
      "AutoOffsetReset": "Earliest",
      "EnablePartitionEof": false,
      "AllowAutoCreateTopics": false,
      "EnableAutoCommit": false
    },
    "ReplicatingConsumer": {
      "ConsumerGroupID": "messaging-replicating-consumer",
      "AutoOffsetReset": "Earliest",
      "EnablePartitionEof": false,
      "AllowAutoCreateTopics": false,
      "EnableAutoCommit": false
    },
    "ReplicatingProducer": {
      "ProducerID": "replicating-producer",
      "Acks": "All",
      "LingerMs": 1.0,
      "MessageTimeoutMs": 300000,
      "MessageSendMaxRetries": 8
    },
    "ServerID": "s1",
    "TopicMessagesByReceiver": "messages-by-receiver",
    "TopicMessagesBySender": "messages-by-sender"
  },
  "Jwt": {
    "Secret": "secret that is long and hard to guess",
    "Issuer": "issuer",
    "Audience": "audience"
  },
  "OtelSampling": {
    "Strategy": "Probability",
    "Probability": 0.1
  },
  "Jaeger": {
    "AgentHost": "localhost",
    "AgentPort": 6831,
    "ExportProcessorType": "Batch",
    "BatchExportScheduledDelayMillis": 2000
  }
}