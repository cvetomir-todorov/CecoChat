{
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "StateDB": {
    "ContactPoints": [
      "192.168.0.101"
    ],
    "LocalDC": "Europe",
    "SocketConnectTimeout": "00:00:05.0",
    "ExponentialReconnectPolicy": true,
    "ExponentialReconnectPolicyBaseDelay": "00:00:00.100",
    "ExponentialReconnectPolicyMaxDelay": "00:00:05.0"
  },
  "Backplane": {
    "Kafka": {
      "BootstrapServers": [
        "localhost:9091",
        "localhost:9092"
      ]
    },
    "StateConsumer": {
      "ConsumerGroupID": "state-consumer",
      "AutoOffsetReset": "Earliest",
      "EnablePartitionEof": false,
      "AllowAutoCreateTopics": false,
      "EnableAutoCommit": false
    },
    "MessagesTopicName": "messages"
  },
  "Jwt": {
    "Secret": "secret that is long and hard to guess",
    "Issuer": "issuer",
    "Audience": "audience"
  }
}
