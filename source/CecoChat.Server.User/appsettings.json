{
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "UserDB": {
    "Seed": false,
    "Connect": {
      "ConnectionString": "Host=localhost;Port=5433;Database=users;Username=cecochat_dev;Password=secret;"
    },
    "HealthTimeout": "00:00:05.0"
  },
  "Jwt": {
    "Secret": "secret that is long and hard to guess",
    "Issuer": "issuer",
    "Audience": "audience"
  },
  "OtelSampling": {
    "Strategy": "Probability",
    "Probability": 0.1
  },
  "Jaeger": {
    "AgentHost": "localhost",
    "AgentPort": 6831,
    "ExportProcessorType": "Batch",
    "BatchExportScheduledDelayMillis": 2000
  },
  "Prometheus": {
    "ScrapeEndpointPath": "/metrics",
    "ScrapeResponseCacheDurationMilliseconds": 0
  }
}
